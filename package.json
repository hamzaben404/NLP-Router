{
  "name": "nlp-router",
  "version": "1.0.0",
  "description": "MVP router that turns a French prompt into a structured action + slots.",
  "type": "module",
  "license": "ISC",
  "author": "",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "dev": "vitest watch",
    "check": "npm run lint && npm run typecheck && npm run test"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.8.1",
    "vitest": "^3.2.4"
  }
}
